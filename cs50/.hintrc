{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content",
          "height: fit-content",
          "text-underline-offset",
          "user-select",
          "counter-set",
          "@property",
          "@property"
        ]
      }
    ],
    "disown-opener": "off",
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ]
  }
}